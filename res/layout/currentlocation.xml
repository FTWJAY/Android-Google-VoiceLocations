        <Button android:id="@+id/thisLocation" android:layout_height="wrap_content" android:drawableLeft="@drawable/blue_dot_circle" android:layout_width="wrap_content" android:gravity="center" android:layout_above="@+id/ad"></Button> 
                 <com.admob.android.ads.AdView  
